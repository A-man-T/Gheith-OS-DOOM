set -e

UTCS_OPT=-O3 make clean the_kernel $1 $1.data

#-d cpu,guest_errors,int,cpu_reset

time `make qemu_cmd` `make qemu_config_flags` \
             -no-reboot \
             --display sdl \
             -serial mon:stdio \
             --monitor none \
             -d guest_errors \
             -D qemu.log \
             -drive file=kernel/build/kernel.img,index=0,media=disk,format=raw \
             -drive file=$1.data,index=1,media=disk,format=raw || true